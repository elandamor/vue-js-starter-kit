{
  "name": "vue-js",
  "version": "1.0.0",
  "description": "Starter Kit for vue.js based websites.",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch:all",
    "bump": "npm version patch",
    "imagemin": "imagemin static/images",
    "icons": "npm run icons:black",
    "icons:black": "svgo -f static/images/icons/black && svg-sprite-generate -d static/images/icons/black -o static/images/icons/black/icons.svg",
    "autoprefixer": "postcss -u autoprefixer static/css/*.css -d static/css",
    "uglify": "uglifyjs static/js/bootstrap.es5.js -o static/js/bootstrap.es5.js -c",
    "clean-css": "node ./_build/clean-css.js",
    "mkdir-css": "mkdirp static/css",
    "babel:js-es6": "rollup -c && babel static/js/bootstrap.js --presets babili --out-file static/js/bootstrap.es6.js",
    "babel:js-es5": "babel static/js/bootstrap.es6.js --presets es2015 --out-file static/js/bootstrap.es5.js",
    "build:css": "npm run mkdir-css && node-sass _dev/sass/ -o static/css && npm run autoprefixer",
    "build:js": "npm run babel:js-es6 && npm run babel:js-es5 && npm run uglify",
    "build:images": "npm run imagemin && npm run icons",
    "build-resource-list": "node ./_build/build-resource-list.js",
    "build:all": "npm run build:css && npm run webpack && npm run build:images && npm run build-resource-list",
    "onchange:js": "onchange 'static/js/bootstrap.js' -- npm run build:js",
    "watch:css": "npm run mkdir-css && node-sass -w _dev/sass/ -o static/css",
    "watch:js": "onchange '_dev/js/*.js' -- npm run webpack",
    "watch:all": "npm run build:all && parallelshell \"npm run watch:css\" \"npm run watch:js\"",
    "postinstall": "npm run watch:all",
    "webpack": "webpack -p --progress --verbose",
    "release": "npm run bump && npm run build:all",
    "serve": "python -m SimpleHTTPServer 3000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elandamor/vue-js-starter-kit"
  },
  "author": "Thandolwethu Mpofu",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-cli": "^6.18.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-preset-babili": "0.1.4",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2017": "^6.16.0",
    "browser-sync": "^2.18.2",
    "clean-css": "^4.1.9",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.28.0",
    "imagemin-cli": "^3.0.0",
    "imageoptim-cli": "^1.14.8",
    "jinja-loader": "0.0.8",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.0",
    "nunjucks": "^3.0.0",
    "onchange": "^3.0.2",
    "parallelshell": "^3.0.2",
    "postcss": "^6.0.14",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.9",
    "preload-webpack-plugin": "^2.1.1",
    "rollup": "^0.53.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-cleanup": "^2.0.0",
    "rollup-watch": "^4.3.1",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.19.1",
    "svg-sprite-generator": "0.0.7",
    "svgo": "^1.0.3",
    "uglify-js": "^3.3.4",
    "url-loader": "^0.6.2",
    "vue": "^2.2.1",
    "vue-loader": "^13.6.2",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.2.1",
    "walk": "^2.3.9",
    "webpack": "^3.10.0"
  }
}
